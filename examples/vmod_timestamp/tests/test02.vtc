varnishtest "now()"

server s1 {} -start

varnish v1 -vcl+backend {
    import timestamp from "${vmod}";
    import std;
    import vtc;

    sub vcl_recv {
        return (synth(200));
    }

    sub vcl_synth {
        set resp.http.diff = timestamp.now() - now;
    }
} -start

client c1 {
    txreq
    rxresp
    expect resp.http.diff <= 0.1
} -run
